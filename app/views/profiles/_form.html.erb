.<%= form_for(@profile) do |f| %>
<div class="field">
    <%= f.label "NAME" %><br>
    <%= f.text_field :user_name %>
  </div>
  <div class="field">
  <%= f.label "性別" %><br>
  <%= f.collection_select :sex_id, Sex.all, :id, :group %>
 </div>

<div class="field">
    <%= f.label "自己紹介" %><br>
    <%= f.text_area :self_introduction%>
  </div>
  <div class="field">
  <%= f.label "画像" %><br>
    <%= f.file_field :photo %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
