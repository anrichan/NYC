<div class="adminprofiles_new">
  <div class="adminprofiles_new_contents">
    <%= form_for(@adminprofile) do |f| %>
      <div class="field">
        <%= f.label "admin_name" %><br>
        <%= f.text_field :admin_name %>
      </div>
      <div class="field">
        <%= f.label "like_location" %><br>
        <%= f.text_area :like_location %>
     </div>

<div class="field">
    <%= f.label "your self introduction" %><br>
    <%= f.text_area :admin_self_introduction %>
  </div>
  <div class="field">
  <%= f.label "myphoto" %><br>
    <%= f.file_field :myphoto %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>
</div>



